# Backend Environment Variables for Vercel

# MongoDB Connection String (from MongoDB Atlas)
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/ssbms?retryWrites=true&w=majority

# JWT Secret (use a strong random string)
JWT_SECRET=your_super_secret_jwt_key_here

# JWT Expiration Time
JWT_EXPIRE=7d

# Node Environment
NODE_ENV=production

# Frontend URL (your deployed frontend URL on Vercel)
FRONTEND_URL=https://your-frontend-app.vercel.app
